class Solution {
    public int maxProfit(int[] prices) {
        int maxdiff =0;
       for(int i=0;i<prices.length;i++)
        {
          for(int j=i+1;j<prices.length;j++)
          {
              if(prices[i]<prices[j])
              {
                  int diff = prices[j]-prices[i];
                  if (diff>maxdiff)
                  {
                      maxdiff = diff;
                  }
                   
              }
          }  
        }
        return maxdiff;
        
    }
}
